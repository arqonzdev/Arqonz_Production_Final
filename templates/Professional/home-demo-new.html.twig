    <script>

        // Initially position each box and apply colors
        gsap.set(".box", {
            // backgroundColor: (i) => colors[i % colors.length],
            x: (i) => i * 340 // Adjusted for 200px width of each box
        });

        const TestiAnimation = gsap.to(".box", {
            duration: 4, // Adjusted for smoother movement with larger boxes
            ease: "none",
            x: "+=240", // Move each box 800px to the right (width of all 4 boxes)
            modifiers: {
                x: gsap.utils.unitize(x => parseFloat(x) % 3400) // Loop within 800px range
            },
            repeat: -1,
            repeatRefresh: true,
            // repeatDelay: 1
        });

        // Pause animation on hover
        document.querySelectorAll('.IFHTestimonialBox').forEach((box) => {
            box.addEventListener('mouseenter', () => {
                TestiAnimation.pause(); // Pause the animation
            });

            box.addEventListener('mouseleave', () => {
                TestiAnimation.play(); // Resume the animation
            });

            // Pause on touch for mobile
            box.addEventListener('touchstart', () => {
                TestiAnimation.pause(); // Pause the animation
            });

            box.addEventListener('touchend', () => {
                TestiAnimation.play(); // Resume the animation
            });
        });
    </script>

