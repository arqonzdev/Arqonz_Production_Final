                                        <h3>{{ architect.getCompanyName() }}</h3>
                                    </div>
                                </div>
                                
                                <div class="ProfessionalStatsCon">
                                    <div class="ProfessionalStat">
                                        <p class="stat">{{ architect.getYearEstablished() }}</p>
                                        <p class="label">Estd. Year</p>
                                    </div>
                                    <div class="ProfessionalStat">
                                        <p class="stat">{{ architect.getProjects() |length }}</p>
                                        <p class="label">Projects</p>
                                    </div>
                                    <div class="ProfessionalStat">
                                        <p class="stat">{{ architect.getProRating() }}</p>
                                        <p class="label">Rating</p>
                                    </div>
                                </div>


                            </div>

                            <div class="Row2">
                                <div class="ProfessionalProfileDesc">
                                    <p>
                                        {{ architect.description|length > 120 ? architect.description|slice(0, 120) ~ '...' : architect.description }} 
                                        <a href="/designer/portfolio/{{ architect.getKey() }}" class="seeMoreProf">See More</a>
                                    </p>
                                </div>
                            </div>
                            {% set projects = architect.getProjects() %}
                            <div class="Row3">
                                <div class="ProfessionalProjects">
                                    <div class="ProjCount">
                                        
                                        <p>All Projects ({{ architect.getProjects() |length }})</p>
                                    </div>
                                    
                                    <div class="ProjectsList">
                                        {% for project in projects|slice(0, 2) %}
                                            <div class="HomeProjCard">
                                                <div class="ProjectimgContainer">
                                                    {% if project.ProjectGallery is not empty %}
                                                        {% set images = project.ProjectGallery.getItems() %}
                                                        {% if images|length > 0 %}
                                                            <img src="{{ images[0].getThumbnail('ProjectFeaturedThumbnail') }}" alt="">
                                                        {% endif %}
                                                    {% else %}
                                                        <img src="/static/images/no-image-found.png" width="265px" height="171px" alt="Project Thumbnail">
                                                    {% endif %}
                                                    <div class="HomeProjectName">
                                                        <h3>{{ project.getProjectName() }}</h3>
                                                        <p>{{ project.getLocation() }}</p>
                                                    </div>
                                                </div>
                                            </div>

                                            
                                        {% endfor %}

                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Get all the .Professionalfeatured elements
            const professionalFeaturedItems = document.querySelectorAll('.Professionalfeatured');
        
            // Add click event listener to each item
            professionalFeaturedItems.forEach(item => {
                item.addEventListener('click', function() {
                    // Remove "FeaturedSelected" class from any element that has it and add "FeaturedUnSelected"
                    professionalFeaturedItems.forEach(feature => {
                        feature.classList.remove('FeaturedSelected');
                        feature.classList.add('FeaturedUnSelected');
                    });
        
                    // Add "FeaturedSelected" class to the clicked element and remove "FeaturedUnSelected"
                    this.classList.add('FeaturedSelected');
                    this.classList.remove('FeaturedUnSelected');
                });
            });
        });
    </script>



    


    

