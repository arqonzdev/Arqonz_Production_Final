    <script>
        document.addEventListener("DOMContentLoaded", () => {
            const slides = document.querySelectorAll('.HeroSectionSlide');
            let currentSlide = 0;
    
            // Function to play animation
            function animateSlide(slide) {
                const slideTextContent = slide.querySelector('.SlideTextContent');
                const label = slideTextContent.querySelector('.SlideTextlable');
                const heading = slideTextContent.querySelector('h2');
                const otherContent = slideTextContent.querySelectorAll('p, .SlideCTAButton');
    
                // Reset initial positions for animations
                gsap.set(label, { y: -30, opacity: 0 });
                gsap.set(heading, { y: -30, opacity: 0 });
                gsap.set(otherContent, { y: 30, opacity: 0 });
    
                // Animate all elements at the same time
                gsap.to([label, heading], {
                    y: 0,
                    opacity: 1,
                    duration: 0.5,
                    ease: 'power2.out',
                });
                gsap.to(otherContent, {
                    y: 0,
                    opacity: 1,
                    duration: 0.5,
                    ease: 'power2.out',
                });
            }
    
            // Function to change slides
            function changeSlide() {
                // Hide current slide
                slides[currentSlide].classList.remove('active');
    
                // Move to next slide
                currentSlide = (currentSlide + 1) % slides.length;
    
                // Show next slide
                slides[currentSlide].classList.add('active');
    
                // Trigger animation on the new slide
                animateSlide(slides[currentSlide]);
            }
    
            // Initialize first slide
            slides[currentSlide].classList.add('active');
            animateSlide(slides[currentSlide]);
    
            // Auto-slide every 5 seconds
            setInterval(changeSlide, 5000);
        });
    </script>

    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // Initialize GSAP ScrollTrigger
            

            // Change menu link color and logo visibility
            const menuLinks = document.querySelectorAll('.Parent-Menu > li > a');
            const LoginLink = document.querySelectorAll('.loginsignUp .login a');
            const blackLogo = document.getElementById('BlackLogo');
            const whiteLogo = document.getElementById('WhiteLOGO');

            // Animation to trigger when HeroNewSection is in view
            ScrollTrigger.create({
                trigger: ".HeroNewSection",
                start: "-100px top",
                end: "bottom top",
                onEnter: () => {
                    // Set link color to white
                    menuLinks.forEach(link => link.style.color = "white");
                    LoginLink.forEach(link => link.style.color = "white");

                    // Swap logos
                    blackLogo.style.display = "none";
                    whiteLogo.style.display = "block";
                },
                onLeave: () => {
                    // Revert to default styles
                    menuLinks.forEach(link => link.style.color = "");
                    LoginLink.forEach(link => link.style.color = "");
                    blackLogo.style.display = "block";
                    whiteLogo.style.display = "none";
                },
                onLeaveBack: () => {
                    // Revert to default styles when scrolling back
                    menuLinks.forEach(link => link.style.color = "");
                    LoginLink.forEach(link => link.style.color = "");
                    blackLogo.style.display = "block";
                    whiteLogo.style.display = "none";
                },
                onEnterBack: () => {
                    // Apply white color and logo swap when scrolling back into section
                    menuLinks.forEach(link => link.style.color = "white");
                    LoginLink.forEach(link => link.style.color = "white");
                    blackLogo.style.display = "none";
                    whiteLogo.style.display = "block";
                },
                markers: false // Set to true for debugging, false for production
            });
        });
    </script>

