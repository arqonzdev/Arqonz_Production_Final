            <script>
                document.addEventListener("DOMContentLoaded", () => {
                    const slides = document.querySelectorAll('.HeroSectionSlide');
                    let currentSlide = 0;
            
                    // Function to play animation
                    function animateSlide(slide) {
                        const slideTextContent = slide.querySelector('.SlideTextContent');
                        const label = slideTextContent.querySelector('.SlideTextlable');
                        const heading = slideTextContent.querySelector('h2');
                        const otherContent = slideTextContent.querySelectorAll('p, .SlideCTAButton');
            
                        // Reset initial positions for animations
                        gsap.set(label, { y: -30, opacity: 0 });
                        gsap.set(heading, { y: -30, opacity: 0 });
                        gsap.set(otherContent, { y: 30, opacity: 0 });
            
                        // Animate all elements at the same time
                        gsap.to([label, heading], {
                            y: 0,
                            opacity: 1,
                            duration: 0.5,
                            ease: 'power2.out',
                        });
                        gsap.to(otherContent, {
                            y: 0,
                            opacity: 1,
                            duration: 0.5,
                            ease: 'power2.out',
                        });
                    }
            
                    // Function to change slides
                    function changeSlide() {
                        // Hide current slide
                        slides[currentSlide].classList.remove('active');
            
                        // Move to next slide
                        currentSlide = (currentSlide + 1) % slides.length;
            
                        // Show next slide
                        slides[currentSlide].classList.add('active');
            
                        // Trigger animation on the new slide
                        animateSlide(slides[currentSlide]);
                    }
            
                    // Initialize first slide
                    slides[currentSlide].classList.add('active');
                    animateSlide(slides[currentSlide]);
            
                    // Auto-slide every 5 seconds
                    setInterval(changeSlide, 5000);
                });
            </script>
