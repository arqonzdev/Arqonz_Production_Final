            <script>
                // Import GSAP
                document.addEventListener("DOMContentLoaded", () => {
                    const openIcons = document.querySelectorAll(".FAQOpenIco");

                    openIcons.forEach(icon => {
                        icon.addEventListener("click", () => {
                            const faqElement = icon.closest(".FAQele");
                            const answer = faqElement.querySelector(".FAQanswer");
                            
                            // Close all other answers
                            document.querySelectorAll(".FAQanswer.open").forEach(openAnswer => {
                                if (openAnswer !== answer) {
                                    gsap.to(openAnswer, { height: 0, opacity: 0, duration: 0.3, onComplete: () => openAnswer.classList.remove("open") });
                                }
                            });

                            // Toggle current answer
                            if (answer.classList.contains("open")) {
                                gsap.to(answer, { height: 0, opacity: 0, duration: 0.3, onComplete: () => answer.classList.remove("open") });
                                answer.style.display = "none";
                            } else {
                                answer.style.display = "block"; // Ensure the element is block for GSAP height calculation
                                const contentHeight = answer.scrollHeight; // Get the actual height
                                gsap.fromTo(answer, { height: 0, opacity: 0 }, { height: contentHeight, opacity: 1, duration: 0.5, onComplete: () => answer.classList.add("open") });
                            }
                        });
                    });
                });
            </script>


