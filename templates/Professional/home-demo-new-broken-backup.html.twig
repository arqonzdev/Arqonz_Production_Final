        /* Modern Hero Section Styles */
        .modern-hero-section {
            background: linear-gradient(135deg, #f8fffe 0%, #f0fdfa 100%);
            position: relative;
            overflow: hidden;
        }
        
        .modern-hero-section::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100"><defs><pattern id="grain" width="100" height="100" patternUnits="userSpaceOnUse"><circle cx="25" cy="25" r="1" fill="%2300A79D" opacity="0.1"/><circle cx="75" cy="75" r="1" fill="%230097A7" opacity="0.1"/><circle cx="50" cy="10" r="0.5" fill="%2300A79D" opacity="0.05"/><circle cx="10" cy="60" r="0.5" fill="%230097A7" opacity="0.05"/></pattern></defs><rect width="100" height="100" fill="url(%23grain)"/></svg>');
            opacity: 0.3;
            z-index: 1;
        }
        
        .hero-content {
            position: relative;
            z-index: 2;
        }
        
        .hero-title {
            color: #1a1a1a;
            line-height: 1.2;
        }
        
        .hero-badge .badge {
            background: linear-gradient(135deg, #00A79D 0%, #0097A7 100%) !important;
            border: none;
            font-size: 0.9rem;
            font-weight: 500;
        }
        
        .feature-item {
            transition: all 0.3s ease;
            border: 1px solid rgba(0, 167, 157, 0.1);
        }
        
        .feature-item:hover {
            transform: translateY(-2px);
            box-shadow: 0 8px 25px rgba(0, 167, 157, 0.15);
            border-color: rgba(0, 167, 157, 0.2);
        }
        
        .hero-image-container {
            position: relative;
            z-index: 2;
        }
        
        .floating-stats {
            z-index: 3;
        }
        
        .stat-card {
            animation: float 6s ease-in-out infinite;
            transition: all 0.3s ease;
        }
        
        .stat-card:nth-child(2) {
            animation-delay: -3s;
        }
        
        @keyframes float {
            0%, 100% { transform: translateY(0px); }
            50% { transform: translateY(-10px); }
        }
        
        .btn-primary {
            background: linear-gradient(135deg, #00A79D 0%, #0097A7 100%);
            border: none;
            font-weight: 600;
            transition: all 0.3s ease;
        }
        
        .btn-primary:hover {
            background: linear-gradient(135deg, #0097A7 0%, #00838F 100%);
            transform: translateY(-2px);
            box-shadow: 0 8px 25px rgba(0, 167, 157, 0.3);
        }
        
        .btn-outline-primary {
            border: 2px solid #00A79D;
            color: #00A79D;
            font-weight: 600;
            transition: all 0.3s ease;
        }
        
        .btn-outline-primary:hover {
            background: #00A79D;
            border-color: #00A79D;
            transform: translateY(-2px);
            box-shadow: 0 8px 25px rgba(0, 167, 157, 0.3);
        }
        
        /* Responsive Design */
        @media (max-width: 991.98px) {
            .hero-title {
                font-size: 2.5rem !important;
            }
            
            .floating-stats {
                display: none;
            }
            
            .hero-cta {
                justify-content: center;
            }
        }
        
        @media (max-width: 767.98px) {
            .hero-title {
                font-size: 2rem !important;
            }
            
            .hero-description {
                font-size: 1rem;
            }
            
            .feature-item {
                margin-bottom: 1rem;
            }
        }
        
        /* Header Styles */
        .DashboardRow {
            position: fixed;
            top: 0;
            background-color: rgba(255, 255, 255, 0.95);
            backdrop-filter: blur(10px);
            border-bottom: 1px solid rgba(0, 167, 157, 0.1);
            z-index: 1000;
        }
        
        .HeroSearch {
            border: 0px !important;
        }
        
        .menuopen img {
            filter: invert(1);
        }
        
        .menuclose img {
            filter: invert(1);
        }
        
        @media (max-width: 724px) {
            .Parent-Menu > li > a {
                color: #000 !important;
            }
            .SlideTextContent h2 {
                font-size: 42px;
                line-height: 3.6rem;
            }
            .HeroSearch {
                display: none;
            }
        }
    </style>
    
