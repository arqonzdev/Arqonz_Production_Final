<!DOCTYPE html>
<html lang="en">
<head>
  <!-- Required Meta Tags -->
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <!-- SEO Meta Tags -->
  <meta name="keywords" content="ArQonZ Deal Desk - Book Slots">
  <meta name="author" content="ArQonZ Global">

  <!-- Title -->
  <title>{{ builder.getname() }}</title>

  <!-- Bootstrap 5 CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">

  <!-- Google Fonts (Optional) -->
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;600&display=swap" rel="stylesheet">

  <!-- Favicon -->
  <link rel="icon" type="image/png" href="favicon.png">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet">
  
  <!-- Custom CSS -->
  <link rel="stylesheet" href="/static/css/dealdeskdetails.css">


</head>
<body>
    <div class="FullContentheight">
        <!-- Navigation Header -->
        <nav class="navbar navbar-expand-lg" style="border-bottom: 1px solid #fff;">
            <div class="container">
                <a class="navbar-brand" href="#">
                    <div class="logo-icon">
                        <img src="/static/images/deal-desk/assets/dealdesk-Logo.png" alt="">
                    </div>
                    
                </a>
                
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav mx-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="https://arqonz.com/deal-desk/">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="https://arqonz.com/about-us">About Us</a>
                        </li>
                        <!-- <li class="nav-item">
                            <a class="nav-link" href="#">Careers</a>
                        </li> -->
                    </ul>
                    <a href="https://arqonz.com/contact-us" class="btn btn-contact">Contact Us</a>
                </div>
            </div>
        </nav>

        <!-- Hero Section -->
        <div class="hero-section">
            <!-- Construction site background image will be applied via CSS -->
        </div>

        <style>
            .hero-section {
                background-image: url('{{ builder.getcoverpic().getFullPath() }}');
            }
        </style>

        <!-- Main Content -->
        <div class="main-content">
            <div class="container">
                <!-- Company Profile Section -->
                <div class="company-profile">
                    <div class="row">
                        <div class="col-lg-3 col-md-4 text-center text-md-start ProfileImageCont">
                            <div class="company-logo mx-auto mx-md-0">
                                <img src="{{ builder.getProfileLogo().getFullPath() }}" alt="">
                            </div>
                        </div>
                        <div class="col-lg-9 col-md-8" style="min-height: 10rem;display: flex;flex-direction: column;justify-content: center;">
                            <h1 class="mb-3">{{ builder.getname() }}</h1>
                            <div class="company-tags">
                                <span class="tag">Construction</span>
                                <span class="tag">Developer</span>
                                <span class="tag">Architecture</span>
                            </div>
                        </div>
                    </div>

                    <!-- Navigation Pills -->
                    <ul class="nav nav-pills" id="companyTabs" role="tablist">
                        <li class="nav-item" role="presentation">
                            <button class="nav-link active" id="overview-tab" data-bs-toggle="pill" data-bs-target="#overview" type="button" role="tab">Overview</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link" id="meeting-tab" data-bs-toggle="pill" data-bs-target="#meeting" type="button" role="tab">Meeting Slot</button>
                        </li>
                    </ul>

                    <!-- Tab Content -->
                    <div class="tab-content" id="companyTabsContent">
                        <div class="tab-pane fade show active" id="overview" role="tabpanel">
                            <div class="row">
                                <div class="col-lg-3">
                                    <h3 class="mb-4">Our Story</h3>
                                   
                                </div>
                                <div class="col-lg-9">
                                    
                                    <p class="text-muted" style="font-weight: 500;">
                                        {{ builder.getDescription() }}
                                    </p>
                                </div>
                            </div>

                            
                        </div>
                        <div class="tab-pane fade" id="meeting" role="tabpanel">
                            <div class="text-center py-5">


                                <!-- Booking Modal -->
                                <div class="booking-modal-overlay" id="bookingModal">
                                    <div class="booking-modal">
                                        <h2 class="text-center mb-4">Available Time Slots</h2>
                                        <p class="text-center">Select a time slot to meet with {{ builder.getName() }}. It Costs <b>1 credit</b> to book a slot.</p>
                                        
                                        <div class="time-slots-container">
                                        {% for slot in timeSlots.time_slots %}
                                            <div class="time-slot {% if slot.availability %}available{% else %}unavailable{% endif %}" 
                                                data-slot="{{ slot.s_no }}"
                                                data-time="{{ slot.time_slot }}"
                                                {% if not slot.availability %}data-bookedby-email="{{ slot.bookedby_email }}"{% endif %}
                                                {% if not slot.availability %}disabled{% endif %}>
                                                <strong>{{ slot.time_slot }}</strong>
                                                <div>
                                                    {% if slot.availability %}
                                                        Available
                                                    {% else %}
                                                        Booked {% if slot.bookedby_email == app.user.email %}(by You){% endif %}
                                                    {% endif %}
                                                </div>
                                            </div>
                                        {% endfor %}
                                        
                                        
                                    </div>
                                        
                                        <div class="booking-form" id="bookingForm">
                                            <h4>Confirm Your Booking</h4>
                                            <p>You're booking: <strong id="selectedTimeSlot"></strong></p>
                                            
                                            
                                            
                                            
                                            <input type="hidden" id="userEmail" value="user@example.com" placeholder="Enter your email address" required>
                                            
                                            
                                            <button class="confirm-booking-btn" id="confirmBookingBtn">Confirm Booking</button>
                                        </div>
                                    </div>
                                </div>

                                <script>
    
                                    document.addEventListener('DOMContentLoaded', function() {
                                        const bookingModal = document.getElementById('bookingModal');
                                        
                                        
                                        const timeSlots = document.querySelectorAll('.time-slot');
                                        const bookingForm = document.getElementById('bookingForm');
                                        const selectedTimeSlot = document.getElementById('selectedTimeSlot');
                                        const confirmBookingBtn = document.getElementById('confirmBookingBtn');
                                        const userEmail = "{{ app.user.email ?? '' }}";
                                        
                                        let selectedSlot = null;
                                        let actionType = 'book'; // can be 'book', 'cancel', or 'reschedule'
                                        let oldSlotForReschedule = null;
                                        
                                        
                                        
                                        
                                        // Handle slot selection
                                        timeSlots.forEach(slot => {
                                            slot.addEventListener('click', function() {
                                                if (this.classList.contains('unavailable')) {
                                                    const slotEmail = this.dataset.bookedbyEmail || '';
                                                    
                                                    // If this is the user's own booking, show cancel/reschedule options
                                                    if (slotEmail === userEmail) {
                                                        showCancelRescheduleOptions(this);
                                                    }
                                                    return;
                                                }
                                                
                                                if (actionType === 'reschedule') {
                                                    // For reschedule, select the new slot
                                                    document.querySelectorAll('.time-slot').forEach(s => {
                                                        s.classList.remove('selected');
                                                    });
                                                    
                                                    this.classList.add('selected');
                                                    selectedSlot = {
                                                        number: this.dataset.slot,
                                                        time: this.dataset.time
                                                    };
                                                    
                                                    // Show confirmation for reschedule
                                                    showRescheduleConfirmation(oldSlotForReschedule, selectedSlot);
                                                } else {
                                                    // Normal booking flow
                                                    document.querySelectorAll('.time-slot').forEach(s => {
                                                        s.classList.remove('selected');
                                                    });
                                                    
                                                    this.classList.add('selected');
                                                    selectedSlot = {
                                                        number: this.dataset.slot,
                                                        time: this.dataset.time
                                                    };
                                                    
                                                    bookingForm.classList.add('active');
                                                    selectedTimeSlot.textContent = selectedSlot.time;
                                                    confirmBookingBtn.classList.add('active');
                                                    confirmBookingBtn.textContent = 'Confirm Booking';
                                                }
                                            });
                                        });
                                        
                                        // Handle booking confirmation
                                        confirmBookingBtn.addEventListener('click', function() {
                                            if (actionType === 'book') {
                                                bookSlot();
                                            }
                                        });
                                        
                                        function showCancelRescheduleOptions(slotElement) {
                                            const slotNumber = slotElement.dataset.slot;
                                            const slotTime = slotElement.dataset.time;
                                            
                                            Swal.fire({
                                                title: 'Your Booked Slot',
                                                html: `You have booked <strong>${slotTime}</strong>. What would you like to do?`,
                                                showDenyButton: true,
                                                showCancelButton: true,
                                                confirmButtonText: 'Reschedule',
                                                denyButtonText: 'Cancel Booking',
                                                cancelButtonText: 'Close',
                                                icon: 'question'
                                            }).then((result) => {
                                                if (result.isConfirmed) {
                                                    // Reschedule flow
                                                    actionType = 'reschedule';
                                                    oldSlotForReschedule = {
                                                        element: slotElement,
                                                        number: slotNumber,
                                                        time: slotTime
                                                    };
                                                    
                                                    // Highlight available slots
                                                    document.querySelectorAll('.time-slot').forEach(s => {
                                                        if (s.classList.contains('available')) {
                                                            s.style.opacity = '1';
                                                            s.style.cursor = 'pointer';
                                                        } else {
                                                            s.style.opacity = '0.5';
                                                            s.style.cursor = 'default';
                                                        }
                                                    });
                                                    
                                                    Swal.fire({
                                                        title: 'Select New Slot',
                                                        text: 'Please select an available slot to reschedule to',
                                                        icon: 'info'
                                                    });
                                                } else if (result.isDenied) {
                                                    // Cancel flow
                                                    Swal.fire({
                                                        title: 'Are you sure?',
                                                        html: `You are about to cancel your booking for <strong>${slotTime}</strong>. <br><br><span style="color: red;">Note: No refund of credits will be given for cancellation.</span>`,
                                                        icon: 'warning',
                                                        showCancelButton: true,
                                                        confirmButtonColor: '#d33',
                                                        confirmButtonText: 'Yes, cancel it!'
                                                    }).then((cancelResult) => {
                                                        if (cancelResult.isConfirmed) {
                                                            cancelSlot(slotNumber);
                                                        }
                                                    });
                                                }
                                            });
                                        }
                                        
                                        function showRescheduleConfirmation(oldSlot, newSlot) {
                                            Swal.fire({
                                                title: 'Confirm Reschedule',
                                                html: `You are rescheduling from <strong>${oldSlot.time}</strong> to <strong>${newSlot.time}</strong>.`,
                                                icon: 'question',
                                                showCancelButton: true,
                                                confirmButtonText: 'Confirm Reschedule'
                                            }).then((result) => {
                                                if (result.isConfirmed) {
                                                    rescheduleSlot(oldSlot.number, newSlot.number);
                                                } else {
                                                    resetSelection();
                                                }
                                            });
                                        }
                                        
                                        function bookSlot() {
                                            // Show loading state
                                            const swalInstance = Swal.fire({
                                                title: 'Processing...',
                                                html: 'Please wait while we book your slot',
                                                allowOutsideClick: false,
                                                didOpen: () => {
                                                    Swal.showLoading();
                                                }
                                            });
                                            
                                            fetch('{{ path('deal_desk_book_slot', {key: builder.getKey()}) }}', {
                                                method: 'POST',
                                                headers: {
                                                    'Content-Type': 'application/x-www-form-urlencoded',
                                                },
                                                body: new URLSearchParams({
                                                    'slotNumber': selectedSlot.number,
                                                })
                                            })
                                            .then(response => response.json())
                                            .then(data => {
                                                swalInstance.close();
                                                
                                                if (data.success) {
                                                    Swal.fire(
                                                        'Success!',
                                                        'Your slot has been booked successfully!',
                                                        'success'
                                                    ).then(() => {
                                                        bookingModal.style.display = 'none';
                                                        window.location.reload();
                                                    });
                                                } else {
                                                    Swal.fire('Error', data.message, 'error');
                                                }
                                            })
                                            .catch(error => {
                                                swalInstance.close();
                                                Swal.fire('Error', 'An error occurred while booking your slot', 'error');
                                                console.error('Error:', error);
                                            });
                                        }
                                        
                                        function cancelSlot(slotNumber) {
                                            const swalInstance = Swal.fire({
                                                title: 'Processing...',
                                                html: 'Cancelling your booking',
                                                allowOutsideClick: false,
                                                didOpen: () => {
                                                    Swal.showLoading();
                                                }
                                            });
                                            
                                            fetch('{{ path('deal_desk_cancel_slot', {key: builder.getKey()}) }}', {
                                                method: 'POST',
                                                headers: {
                                                    'Content-Type': 'application/x-www-form-urlencoded',
                                                },
                                                body: new URLSearchParams({
                                                    'slotNumber': slotNumber,
                                                })
                                            })
                                            .then(response => response.json())
                                            .then(data => {
                                                swalInstance.close();
                                                
                                                if (data.success) {
                                                    Swal.fire(
                                                        'Cancelled!',
                                                        'Your booking has been cancelled.',
                                                        'success'
                                                    ).then(() => {
                                                        bookingModal.style.display = 'none';
                                                        window.location.reload();
                                                    });
                                                } else {
                                                    Swal.fire('Error', data.message, 'error');
                                                }
                                            })
                                            .catch(error => {
                                                swalInstance.close();
                                                Swal.fire('Error', 'An error occurred while cancelling your booking', 'error');
                                                console.error('Error:', error);
                                            });
                                        }
                                        
                                        function rescheduleSlot(oldSlotNumber, newSlotNumber) {
                                            const swalInstance = Swal.fire({
                                                title: 'Processing...',
                                                html: 'Rescheduling your booking',
                                                allowOutsideClick: false,
                                                didOpen: () => {
                                                    Swal.showLoading();
                                                }
                                            });
                                            
                                            fetch('{{ path('deal_desk_reschedule_slot', {key: builder.getKey()}) }}', {
                                                method: 'POST',
                                                headers: {
                                                    'Content-Type': 'application/x-www-form-urlencoded',
                                                },
                                                body: new URLSearchParams({
                                                    'oldSlotNumber': oldSlotNumber,
                                                    'newSlotNumber': newSlotNumber
                                                })
                                            })
                                            .then(response => response.json())
                                            .then(data => {
                                                swalInstance.close();
                                                
                                                if (data.success) {
                                                    Swal.fire(
                                                        'Rescheduled!',
                                                        'Your booking has been rescheduled successfully.',
                                                        'success'
                                                    ).then(() => {
                                                        bookingModal.style.display = 'none';
                                                        window.location.reload();
                                                    });
                                                } else {
                                                    Swal.fire('Error', data.message, 'error');
                                                }
                                            })
                                            .catch(error => {
                                                swalInstance.close();
                                                Swal.fire('Error', 'An error occurred while rescheduling your booking', 'error');
                                                console.error('Error:', error);
                                            });
                                        }
                                        
                                        // Reset selection when modal is closed
                                        function resetSelection() {
                                            selectedSlot = null;
                                            oldSlotForReschedule = null;
                                            actionType = 'book';
                                            bookingForm.classList.remove('active');
                                            confirmBookingBtn.classList.remove('active');
                                            document.querySelectorAll('.time-slot').forEach(s => {
                                                s.classList.remove('selected');
                                                s.style.opacity = '';
                                                s.style.cursor = '';
                                            });
                                        }
                                        
                                        // Add bookedby_email data attribute to all slots during initialization
                                        document.querySelectorAll('.time-slot.unavailable').forEach(slot => {
                                            const slotNumber = slot.dataset.slot;
                                            const timeSlotsData = {{ timeSlots|json_encode|raw }};
                                            const slotData = timeSlotsData.time_slots.find(s => s.s_no == slotNumber);
                                            if (slotData) {
                                                slot.dataset.bookedbyEmail = slotData.bookedby_email || '';
                                            }
                                        });
                                    });
                                </script>

                            </div>
                        </div>
                    </div>

