    <!-- WhatsApp Float Button Styles -->
    <style>
        .whatsapp-float {
            position: fixed;
            width: 60px;
            height: 60px;
            bottom: 40px;
            left: 40px;
            background-color: #25d366;
            color: #FFF;
            border-radius: 50px;
            text-align: center;
            font-size: 30px;
            box-shadow: 2px 2px 3px #999;
            z-index: 100;
            transition: all 0.3s ease;
            cursor: pointer;
            display: flex;
            align-items: center;
            justify-content: center;
            text-decoration: none;
        }
        
        .whatsapp-float:hover {
            background-color: #20c157;
            transform: scale(1.1);
            box-shadow: 3px 3px 6px #999;
        }
        
        .whatsapp-float svg {
            width: 32px;
            height: 32px;
            fill: white;
        }
        
        /* Pulse animation */
        @keyframes pulse {
            0% {
                transform: scale(1);
                box-shadow: 0 0 0 0 rgba(37, 211, 102, 0.7);
            }
            70% {
                transform: scale(1);
                box-shadow: 0 0 0 10px rgba(37, 211, 102, 0);
            }
            100% {
                transform: scale(1);
                box-shadow: 0 0 0 0 rgba(37, 211, 102, 0);
            }
        }
        
        .whatsapp-float {
            animation: pulse 2s infinite;
        }
        
        /* Mobile responsive */
        @media screen and (max-width: 768px) {
            .whatsapp-float {
                width: 50px;
                height: 50px;
                bottom: 20px;
                left: 20px;
                font-size: 24px;
            }
            
            .whatsapp-float svg {
                width: 28px;
                height: 28px;
            }
        }
    </style>
