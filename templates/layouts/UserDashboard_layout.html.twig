    {% if customer.getEmailVerified() == 'false' %}
    <div id="email-verify-banner" class="email-verify-notice">
        <p id="email-verify-text" class="email-verify-text">
          <span id="mainStatusText">Please verify your email address by clicking the verification link sent to your inbox.</span>
          <a href="#" class="email-verify-link">click here to resend</a>.
        </p>
        <span class="email-verify-close-btn" onclick="dismissEmailVerifyBanner()">×</span>
    </div>
    <div id="resend-status" style="display:none; color:green;"></div>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const closeBtn = document.querySelector('.email-verify-close-btn');
            const banner = document.getElementById('email-verify-banner');
            const resendLink = document.querySelector('.email-verify-link');
            const statusDiv = document.getElementById('resend-status');
            const verifytext = document.getElementById('mainStatusText');
            const successtext = document.getElementById('email-verify-text');
        
            // Hide the banner when close button is clicked
            closeBtn.addEventListener('click', function() {
            banner.style.display = 'none';
            });
        
            // Handle resend email link click
            resendLink.addEventListener('click', function(e) {
            e.preventDefault(); // Prevent the default link behavior
        
            // Send AJAX request to the resend route
            fetch('/emailVerifications/resend', {
                method: 'POST',
                headers: {
                'Content-Type': 'application/json',
                'X-Requested-With': 'XMLHttpRequest'
                },
                credentials: 'same-origin' // Include cookies for session
            })
            .then(response => {
                if (response.ok) {
                return response.json(); // Parse the JSON response
                } else {
                throw new Error('Network response was not ok');
                }
            })
            .then(data => {
                if (data.success) {
                // Display success message
                successtext.innerHTML = 'Verification email has been resent successfully!';
                successtext.style.color = 'green';
                } else {
                // Display failure message based on response
                verifytext.innerHTML = data.message || 'Failed to resend verification email.';
                verifytext.style.color = 'red';
                }
                
            })
            .catch(error => {
                // Handle any network or request errors
                verifytext.innerHTML = 'An error occurred. Please try again later.';
                verifytext.style.color = 'red';
                
            });
            });
        });
    </script>
