                        color: #28a745;
                        background: rgba(40, 167, 69, 0.1);
                        padding: 2px 8px;
                        border-radius: 12px;
                        border: 1px solid rgba(40, 167, 69, 0.2);
                    }

                    .profile-progress-container {
                        position: relative;
                        width: 100%;
                        height: 8px;
                        background-color: #e9ecef;
                        border-radius: 10px;
                        overflow: hidden;
                        box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1);
                    }

                    .profile-progress-bar {
                        height: 100%;
                        background: linear-gradient(90deg, #28a745 0%, #20c997 50%, #17a2b8 100%);
                        border-radius: 10px;
                        position: relative;
                        transition: width 0.8s cubic-bezier(0.4, 0, 0.2, 1);
                        box-shadow: 0 1px 3px rgba(40, 167, 69, 0.3);
                    }

                    .profile-progress-bar::after {
                        content: '';
                        position: absolute;
                        top: 0;
                        left: 0;
                        right: 0;
                        bottom: 0;
                        background: linear-gradient(90deg, transparent 0%, rgba(255, 255, 255, 0.3) 50%, transparent 100%);
                        animation: shimmer 2s infinite;
                    }

                    @keyframes shimmer {
                        0% { transform: translateX(-100%); }
                        100% { transform: translateX(100%); }
                    }

                    .profile-completion-status {
                        margin-top: 6px;
                        font-size: 11px;
                        color: #6c757d;
                        text-align: center;
                    }

                    /* Color variations based on completion percentage */
                    .profile-progress-bar.low-completion {
                        background: linear-gradient(90deg, #dc3545 0%, #fd7e14 100%);
                        box-shadow: 0 1px 3px rgba(220, 53, 69, 0.3);
                    }

                    .profile-progress-bar.medium-completion {
                        background: linear-gradient(90deg, #ffc107 0%, #fd7e14 100%);
                        box-shadow: 0 1px 3px rgba(255, 193, 7, 0.3);
                    }

                    .profile-progress-bar.high-completion {
                        background: linear-gradient(90deg, #28a745 0%, #20c997 100%);
                        box-shadow: 0 1px 3px rgba(40, 167, 69, 0.3);
                    }

                    /* Responsive adjustments */
                    @media (max-width: 768px) {
                        .profile-completion-wrapper {
                            margin: 10px 0;
                            padding: 10px 12px;
                        }
                        
                        .profile-completion-title {
                            font-size: 12px;
                        }
                        
                        .profile-completion-percentage {
                            font-size: 11px;
                            padding: 1px 6px;
                        }
                    }
                </style>

                <script>
                    // Function to update the profile completion bar
                    function updateProfileCompletion(percentage) {
                        const progressBar = document.getElementById('progressBar');
                        const completionText = document.getElementById('completionText');
                        const completionStatus = document.getElementById('completionStatus');
                        
                        // Update percentage display
                        completionText.textContent = percentage + '%';
                        
                        // Update progress bar width
                        progressBar.style.width = percentage + '%';
                        
                        // Update color scheme based on completion level
                        progressBar.classList.remove('low-completion', 'medium-completion', 'high-completion');
                        
                        if (percentage < 40) {
                            progressBar.classList.add('low-completion');
                            completionStatus.textContent = "Let's get started! Complete your profile to unlock features.";
                        } else if (percentage < 80) {
                            progressBar.classList.add('medium-completion');
                            completionStatus.textContent = "Great progress! Keep going to complete your profile.";
                        } else {
                            progressBar.classList.add('high-completion');
                            completionStatus.textContent = "Excellent! Your profile is nearly complete.";
                        }
                        
                        if (percentage >= 100) {
                            completionStatus.textContent = "🎉 Profile Complete! You have access to all features.";
                        }
                    }

