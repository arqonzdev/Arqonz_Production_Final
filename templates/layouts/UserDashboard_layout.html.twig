    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const options = document.querySelectorAll('.CustomizeOption');
            options.forEach(option => {
                option.addEventListener('click', function () {
                    const optionType = this.getAttribute('data-option');
                    const customizeProductWrapper = this.closest('.customizeProductWrapper');
                    const productName = customizeProductWrapper.querySelector('.BoqProductName').textContent.trim();

                    // Toggle the display of the clicked option
                    const detailSection = customizeProductWrapper.querySelector(`.customizationDetails[data-type="${optionType}"]`);
                    if (detailSection) {
                        if (detailSection.style.display === 'block') {
                            detailSection.style.display = 'none';
                        } else {
                            const detailSections = customizeProductWrapper.querySelectorAll('.customizationDetails');
                            detailSections.forEach(section => {
                                section.style.display = 'none';
                            });

                            // Fetch the brands if the optionType is 'brands'
                            if (optionType === 'brands') {
                                const xhr = new XMLHttpRequest();
                                xhr.open('POST', '{{ path('fetch_brands_api') }}', true);
                                xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
                                xhr.onload = function () {
                                    if (xhr.status === 200) {
                                        const brands = JSON.parse(xhr.responseText);
                                        let brandList = '<ul>';
                                        brands.forEach(brand => {
                                            brandList += `<li>${brand}</li>`;
                                        });
                                        brandList += '</ul>';
                                        detailSection.innerHTML = brandList;
                                        detailSection.style.display = 'block';
                                    } else {
                                        console.error('Error fetching brands:', xhr.statusText);
                                    }
                                };
                                xhr.onerror = function () {
                                    console.error('Request failed');
                                };
                                xhr.send(`product_type=${encodeURIComponent(productName)}`);
                            } else {
                                detailSection.style.display = 'block';
                            }
                        }
                    }
                });
            });

            

            
        });
    </script>

    <script>
       document.addEventListener('DOMContentLoaded', (event) => {
            // Function to handle click outside the dropdown and notifications
            function handleClickOutside(event) {
                const dropdowns = document.querySelectorAll('.dropdown-content');
                dropdowns.forEach(dropdown => {
                    if (!dropdown.parentElement.contains(event.target)) {
                        dropdown.style.display = 'none';
                    }
                });

                const notifications = document.querySelector('.notifications');
                if (notifications && !document.querySelector('.menunotifcation').contains(event.target)) {
                    notifications.style.display = 'none';
                }
            }

            // Add event listeners to dropdown buttons
            const dropBtns = document.querySelectorAll('.dropbtn');
            dropBtns.forEach(btn => {
                btn.addEventListener('click', function(event) {
                    event.preventDefault();
                    event.stopPropagation();

                    // Hide notifications when any dropdown is clicked
                    const notifications = document.querySelector('.notifications');
                    if (notifications) {
                        notifications.style.display = 'none';
                    }

                    const dropdownContent = this.nextElementSibling;

                    // Hide all other dropdowns
                    const dropdowns = document.querySelectorAll('.dropdown-content');
                    dropdowns.forEach(dropdown => {
                        if (dropdown !== dropdownContent) {
                            dropdown.style.display = 'none';
                        }
                    });

                    // Toggle the display of the current dropdown
                    if (dropdownContent.style.display === 'block') {
                        dropdownContent.style.display = 'none';
                    } else {
                        dropdownContent.style.display = 'block';
                    }
                });
            });

            // Add event listener to notification button
            const notifBtn = document.querySelector('.menunotifcation');
            if (notifBtn) {
                notifBtn.addEventListener('click', function(event) {
                    event.preventDefault();
                    event.stopPropagation();

                    // Hide all dropdowns when the notification button is clicked
                    const dropdowns = document.querySelectorAll('.dropdown-content');
                    dropdowns.forEach(dropdown => {
                        dropdown.style.display = 'none';
                    });

                    const notifications = this.querySelector('.notifications');

                    // Toggle the display of the notifications
                    if (notifications.style.display === 'block') {
                        notifications.style.display = 'none';
                    } else {
                        notifications.style.display = 'block';
                    }
                });
            }

            // Close the dropdown and notifications when clicking outside
            document.addEventListener('click', handleClickOutside);
        });
    </script>

    <script>
        // Wait for the page to fully load
        document.addEventListener("DOMContentLoaded", function () {
            // Select flash messages with the "flash-message" class
            const flashMessage = document.querySelector('.alert-dismissable');
            
            if (flashMessage) {
                // Set a timeout to hide the flash message after 5 seconds
                setTimeout(() => {
                    flashMessage.style.transition = 'opacity 0.5s ease';
                    flashMessage.style.opacity = '0';
                    setTimeout(() => flashMessage.remove(), 500); // Remove it from the DOM
                }, 5000);
            }
        });
    </script>

    
