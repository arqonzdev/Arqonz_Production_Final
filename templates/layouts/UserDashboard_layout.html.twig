                    }

                    .profile-completion-header {
                        display: flex;
                        align-items: center;
                        justify-content: space-between;
                        margin-bottom: 8px;
                    }

                    .profile-completion-title {
                        font-size: 13px;
                        font-weight: 600;
                        color: #495057;
                        margin: 0;
                        display: flex;
                        align-items: center;
                    }

                    .profile-completion-title svg {
                        margin-right: 6px;
                        opacity: 0.7;
